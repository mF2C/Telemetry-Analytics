{
   "version":1,
   "schedule":{
      "type":"simple",
      "interval":"1s"
   },
   "max-failures": 10,
   "workflow":{
      "collect":{
         "metrics":{
            "/intel/*":{
            }
         },
         "publish":[
            {
               "plugin_name":"influxdb",
               "config":{
                  "host":"influxdb",
                  "port":8086,
                  "database":"snap",
                  "user":"snap",
                  "password":"snap"
               }
            }
         ]
      }
   }
}
